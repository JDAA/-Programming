#include<iostream>
using namespace std;
int g(int , int );
int f(int , int);
int h(int, int );
int main(){
    int a,b;
    cout<<"Enter 2 numbers";
    cin>>a;
    cin>>b;
cout<<g(a,b);
}
int g(int x, int y){
    return(x+y);
    
}
int f(int x, int y){
return(x-y);
}
int h(int x, int y){
return(x*y);
}
